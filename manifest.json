{
  "manifest_version": 3,
  "name": "pcc plugin",
  "version": "0.0.1",
  "description": "个人的浏览器plugin",
  "icons": {
    "16": "img/favicon.ico",
    "48": "img/favicon.ico",
    "128": "img/favicon.ico"
  },
  "background": {
    "service_worker": "script/background.js"
  },
  "permissions": [
    "declarativeContent",
    "tabs"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://192.168.1.213:9900/*", "https://capi.xmjztm.com/*"
      ],
      "js": [
        "script/import.js"
      ]
    },
    {
      "matches": [
        "https://www.zhihu.com/*"
      ],
      "js": [
        "module/zhihu/reader/catch.ebook.js"
      ]
    },
    {
      "matches": [
        "https://naotu.baidu.com/*"
      ],
      "js": [
        "script/jquery-3.1.1.js",
        "module/naotu/import-export.js"
      ],
      "css": [
        "module/naotu/import-export.css"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "http://localhost/*",
        "http://127.0.0.1/*",
        "http://192.168.1.33/*"
      ],
      "js": [
        "script/jquery-3.1.1.js",
        "module/dev/autocomplete-username-and-password.js"
      ]
    },
    {
      "matches": [
        "https://mp.weixin.qq.com/*"
      ],
      "js": [
        "script/jquery-3.1.1.js",
        "script/background.js",
        "script/layui/layui.js",
        "module/weixin/mq/mq.js"
      ],
      "css": [
        "module/weixin/mq/mq.css",
        "script/layui/css/layui.css",
        "script/layui/css/modules/laydate/default/laydate.css",
        "script/layui/css/modules/layer/default/layer.css",
        "script/layui/css/modules/code.css"
      ]
    },
    {
      "matches": [
        "https://accounts.google.com/*"
      ],
      "js": [
        "script/jquery-3.1.1.js",
        "module/google/accounts.google.js"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "module/google/google.search.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/module/intercept/forward.js"
      ],
      "matches": [
        "http://192.168.1.213:9900/*", "https://capi.xmjztm.com/*"
      ]
    },
    {
      "resources": [
        "/module/zhihu/reader/catch.ebook.js"
      ],
      "matches": [
        "https://www.zhihu.com/*"
      ]
    }
  ]
}
